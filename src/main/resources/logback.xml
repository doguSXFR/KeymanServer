<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{yyyy-MM-dd HH:mm:ss}]@[%-25.25(%thread)][%-5level{5}][%-36logger{36}] == %msg%n</pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="com.wave" level="DEBUG"/>
    <!--    <logger name="io.netty" level="INFO"/> -->
</configuration>